    "regenerator-runtime": "^0.13.2",
    "resolve-url-loader": "^3.1.0",
    "sass": "^1.32.12",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.12"
  },
  "dependencies": {
    "core-js": "^3.12.1"
